{
    "Authority": {
        "Audience": "https://authserver.com",
        "Authority": "https://authserver.com",
        "RequireHttpsMetadata": true,
        "Scopes": {
            "api.admin": "API Admin - Full Control "
        }
    },
    "Info": {
        "Description": "fwkslabs @ Resume management service",
        "Name": "Resume Service",
        "Version": "1.0.0-beta"
    },
    "Serilog": {
        "Enrich": [
            { "Name": "FromLogContext" },
            { "Name": "WithMachineName" },
            { "Name": "WithEnvironmentName" },
            { "Name": "WithClientIp" },
            { "Name": "WithCorrelationId" }
        ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "Microsoft.EntityFramework": "Information",
                "System": "Information"
            }
        },
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://localhost:5341"
                }
            }
        ]
    }
}